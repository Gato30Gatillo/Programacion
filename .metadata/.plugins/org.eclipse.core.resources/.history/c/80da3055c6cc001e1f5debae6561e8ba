package ejercicio.eningles.exercise16_17;

import ejemplosenclase.ArrayMetodos;

public class Ejercicio16_17 {

	private static int contarPositivos( int[] arrayParam) {
		   int contador=0;
		   
		   for (int i=0;i<arrayParam.length;i++) {
			   if(arrayParam[i]>0) {
				   contador++;
			   }
		   }
		   return contador;
	}
	
	
	public static Integer[] returnaArrayWithPositives(Integer[] arrayParam) {
		
		Integer[] arrayRes;
		
		return arrayRes;
		
	}
		 
	public static void main(String[] args) {
		
		
		Integer[] arrayInt= ArrayMetodos.rellenaArrayAleatoriosWrap(20, 50);
	}
	
	
}
