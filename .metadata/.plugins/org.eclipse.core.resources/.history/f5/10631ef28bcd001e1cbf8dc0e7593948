package ejercicio.eningles.exercise21;

import ejemplosenclase.ArrayMetodos;

public class Ejercicio21 {

	public static int[] DeleteDuplicateElement(int[] arrayParam) {
		
		int[] arrayRes = new int[arrayParam.length];
		int cont=1;
		arrayRes[0]=arrayParam[0];
		boolean repetido=false;
		for (int i=0;i<arrayParam.length;i++) {	
			repetido=false;
			for (int j=0;i<arrayParam.length && repetido==false;j++) {	
				if(arrayRes[j]==arrayParam[i]) {
					repetido=true;
				}
			}
			
			
			if(repetido=false) {
				arrayRes[cont]=arrayParam[i];
				cont++;
				repetido=false;
			}
		}
		
		int[] arrayEnd = new int[cont];
		for (int i=0;i<cont;i++) {
			arrayEnd[i]=arrayRes[i];
		}
		return arrayEnd;
	}
	public static void main(String[] args) {
		
		
		int[] arrayInt= {1,2,3,4,1,3,2};
		
		ArrayMetodos.mostrarArray(arrayInt);
		
		arrayInt=DeleteDuplicateElement(arrayInt);//fallo aqui
		
		ArrayMetodos.mostrarArray(arrayInt);
	}
}
